                <label class="uk-form-label" for="phone-input">Phone</label>
                <input id="phone-input" type="text" class="uk-input" placeholder="Phone">

                <label class="uk-form-label" for="message-input">Message</label>
                <textarea class="uk-textarea" name="message-input" id="message-input" cols="30" rows="10"
                    placeholder="Message">
                </textarea>
                <button>Hello</button>
            </form>
        </div>

        <div>
            <h3>Our Location</h3>
            <p>Company Address</p>
            <p>California Mobility Works
                1355 California Circle Milpitas CA 95035 , USA
                Phone : +1 408-357-4745
                USA E-Mail: info@cmw-us.com</p>
        </div>
    </div>

    <div style="padding-top: 60px;">
        <div id="map" style="height:200px; width:auto;">
        </div>
    </div>





